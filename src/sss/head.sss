.head
	padding: 75px 0

	&__wrap
		display: flex
		align-items: center
		justify-content: space-between

.logo
	display: block

	svg, img
		display: inherit

.h-link
	color: inherit
	border: 1px solid rgba(255,255,255, .3)
	padding: 10px 22px 13px
	display: inline-block
	transition: background-color .3s ease

	&:hover
		background-color: var(--thrid-color)

